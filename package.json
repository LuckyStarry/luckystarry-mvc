{
  "name": "luckystarry-mvc",
  "version": "0.1.0-7",
  "description": "一个轻量的 WebApi 服务",
  "keywords": [
    "mvc"
  ],
  "scripts": {
    "prebuild": "del-cli dist -f",
    "build": "tsc",
    "pretest": "del-cli .nyc_output coverage -f",
    "test": "nyc mocha",
    "testonly": "mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "x": "npm i --registry=https://registry.npm.taobao.org"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "include": [
      "src"
    ],
    "all": true
  },
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh:git@github.com:LuckyStarry/luckystarry-mvc.git"
  },
  "bugs": {
    "email": "starry@vip.qq.com",
    "url": "https://github.com/LuckyStarry/luckystarry-mvc/issues"
  },
  "author": {
    "name": "SUN BO",
    "email": "starry@vip.qq.com",
    "url": "https://www.luckystarry.com"
  },
  "dependencies": {
    "express": "^4.17.1",
    "luckystarry-collections": "^1.0.0",
    "luckystarry-ioc": "^0.2.1",
    "luckystarry-log4ts": "^1.0.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/multer": "^1.4.3",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "del-cli": "^3.0.1",
    "mocha": "^9.1.2",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^9.0.0"
  }
}
